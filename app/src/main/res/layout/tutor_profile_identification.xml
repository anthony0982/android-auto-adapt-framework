<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:a="http://schemas.android.com/apk/res/android"
    a:layout_width="wrap_content"
    a:layout_height="wrap_content" >

    <x.ImageView
        a:id="@+id/tutor_profile_identification_img_icon"
        a:layout_width="34px"
        a:layout_height="43px"
        a:src="@drawable/tutor_profile_identification" />

    <x.TextView
        a:id="@+id/tutor_profile_identification_txt_name"
        a:layout_width="wrap_content"
        a:layout_height="wrap_content"
        a:layout_marginLeft="17px"
        a:layout_toRightOf="@id/tutor_profile_identification_img_icon"
        a:paddingRight="35px"
        a:textColor="#32a8dd"
        a:textSize="38px" />

</RelativeLayout>